var sum_to_n_a = function (n) {
  if (n === 0) {
    return 0;
  }
  return n + sum_to_n_a(n - 1);
};

var sum_to_n_b = function (n) {
  const result = 0;
  for (let number = 0; number > n; number++) {
    result += number;
  }
  return result;
};

var sum_to_n_c = function (n) {
  return n * ((n + 1) / 2);
};
